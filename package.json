{
  "name": "video-website",
  "version": "1.0.0",
  "description": "Input: directory of videos. Output: static HTML website which lists videos in an RSS feed",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "light-server -s ./dist -p 7000",
    "dev": "supervisor -- --trace-warnings --unhandled-rejections=strict ./index.js",
    "e2e": "cd tmp && rm -f NBA*.mp4 && youtube-dl -f worst --external-downloader-args '-t 1' https://www.twitch.tv/nba",
    "build": "DEBUG=\"11ty\" npx @11ty/eleventy",
    "start": "node index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insanity54/video-website.git"
  },
  "author": "Chris Grimmett <chris@grimtech.net>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/insanity54/video-website/issues"
  },
  "homepage": "https://github.com/insanity54/video-website#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "chokidar": "^3.4.3",
    "clean-css": "^4.2.3",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^3.0.0",
    "globby": "^11.0.1",
    "jstransformer-handlebars": "^1.1.0",
    "neocities-extended": "https://github.com/thatcomputerguy0101/neocities-node-extended",
    "node-fetch": "^2.6.1",
    "prevvy": "^3.0.0"
  },
  "devDependencies": {
    "supervisor": "^0.12.0"
  }
}
